{
  "name": "starmade-launcher",
  "version": "3.0.0",
  "description": "New launcher for StarMade",
  "main": "src/main/js/main.js",
  "scripts": {
    "package-mac": "node_modules/.bin/electron-packager . starmade-launcher --overwrite --platform=darwin --arch=x64 --icon=assets/starmade.icns --prune=true --out=release-builds --extra-resource=starmade-launcher.jar --extra-resource=assets",
    "package-win": "node_modules/.bin/electron-packager . starmade-launcher --overwrite --asar=true --platform=win32 --arch=ia32  --icon=assets/starmade.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='StarMade Launcher' --extra-resource=starmade-launcher.jar --extra-resource=assets",
    "package-linux": "node_modules/.bin/electron-packager . starmade-launcher --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/starmade.png --prune=true --out=release-builds --extra-resource=starmade-launcher.jar --extra-resource=assets"
  },
  "author": "Schine, StarLoader Team",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.4.15",
    "electron-packager": "^8.5.2"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garretreichenbach/New-StarMade-Launcher.git"
  },
  "bugs": {
    "url": "https://github.com/garretreichenbach/New-StarMade-Launcher/issues"
  },
  "homepage": "https://github.com/garretreichenbach/New-StarMade-Launcher#readme"
}